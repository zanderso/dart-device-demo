{
  "name": "Dart Device Demo",
  "description": "A Dart Android device access demo",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "version": "0.0.1",
  "offline_enabled": true,
  "icons": {
    "128": "dart_icon.png"
  },
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "http://api.wunderground.com/",
    "http://autocomplete.wunderground.com/"
  ]
}
